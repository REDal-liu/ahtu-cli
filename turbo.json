{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "./dist/**",
                "!./dist/.gitkeep"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    },
    "ui": "tui"
}